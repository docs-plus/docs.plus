{
  "name": "@docs.plus/extension-indent",
  "version": "0.1.1",
  "description": "A Tiptap extension for managing text indentation in documents",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": "Hossein Marzban",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/docs-plus/docs.plus",
    "directory": "packages/extension-indent"
  },
  "scripts": {
    "build": "NODE_ENV=production bunx tsup",
    "build:dev": "bunx tsup",
    "dev": "bunx tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update:packages": "bunx npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tiptap",
    "tiptap extension",
    "indent",
    "outdent",
    "text editor",
    "rich text",
    "prosemirror",
    "docs.plus"
  ],
  "peerDependencies": {
    "@tiptap/core": "^3.10.7",
    "@tiptap/pm": "^3.10.7"
  },
  "devDependencies": {
    "@tiptap/core": "^3.10.7",
    "@tiptap/pm": "^3.10.7",
    "typescript": "^5.9.3",
    "tsup": "^8.5.1",
    "eslint": "^9.39.1"
  }
}
