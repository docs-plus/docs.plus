{
  "name": "@docs.plus/extension-hyperlink",
  "version": "3.0.0",
  "description": "Hyperlink component for docs.plus",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": "Hossein Marzban",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/docs-plus/docs.plus",
    "directory": "packages/extension-hyperlink"
  },
  "scripts": {
    "build": "NODE_ENV=production bunx tsup",
    "build:dev": "bunx tsup",
    "dev": "bunx tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "update:packages": "bunx npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tiptap",
    "tiptap extension",
    "hyperlink",
    "link",
    "docs.plus",
    "prosemirror"
  ],
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "linkifyjs": "^4.3.2"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.6.2",
    "@tiptap/pm": "^3.6.2"
  },
  "devDependencies": {
    "@tiptap/core": "^3.6.2",
    "@tiptap/pm": "^3.6.2",
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
