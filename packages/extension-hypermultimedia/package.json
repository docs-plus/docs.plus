{
  "name": "@docs.plus/extension-hypermultimedia",
  "version": "1.4.0",
  "description": "Insert embeded multimedia components for docs.plus",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": "Hossein Marzban",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/docs-plus/docs.plus",
    "directory": "packages/extension-hypermultimedia"
  },
  "scripts": {
    "build": "NODE_ENV=production bunx tsup",
    "build:dev": "bunx tsup",
    "dev": "bunx tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "update:packages": "bunx npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tiptap",
    "tiptap extension",
    "insert Media",
    "multimedia",
    "youtube",
    "vimeo",
    "soundcloud",
    "twitter",
    "prosemirror",
    "docs.plus"
  ],
  "peerDependencies": {
    "@tiptap/core": "^3.10.7",
    "@tiptap/pm": "^3.10.7"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@tiptap/core": "^3.10.7",
    "@tiptap/pm": "^3.10.7",
    "@eslint/js": "^9.39.1",
    "@types/minimatch": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
