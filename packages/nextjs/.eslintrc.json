{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "next",
      "next/core-web-vitals",
      "prettier",
      "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": "off",
      "no-restricted-syntax": ["error",
          {
            "selector": "CallExpression[callee.object.name='console'][callee.property.name!=/^(info|error)$/]",
            "message": "Unexpected console method."
          }
        ]
      }
}
